section.restaurant-bar-map
  .container.no-gutter
    .map-view-container
      .map-wrapper
        #rb-map(data-service="./services/rb-map-qufu.json")
      .title
          h2.title-name Restaurant and Bars nearby
            span.count
      .detail-container
        .container.no-gutter
          h3.hotel-name SLQF
            span.count (5)
          .row
            // This is a restaurant and bar container 1
            .col-xs-12.col-sm-6.col-md-12
              .restaurant-bar-container(data-hotel-id="SLQF", data-venue-id="SLQF_LOBBYLOUNGE")
                .image-container.lazy-load-wrapper
                  a(href="#")
                    img.lazy-load(src="content/img/content/placeholder.png", data-src="content/img/content/lobster-bar-grill.jpg", alt="")
                    span.icon
                      span.icon-restaurants-bars
                .content-container
                  h2 
                    a(href="#") Sigi's Bar &amp; Grill On The Beach
                  p.restaurant-bar-address Island Shangri-La, Hong Kong Hong Kong
                  p.restaurant-bar-address Hong Kong
                  p.cuisine Grill and Steakhouse
                    span.divider |
                    span.price $$
                  p.offer Offers (2)
                    span.divider |
                    span.price Experiences (1)
                  p.contact-info +
                    span.contact-number 852 12345678
